<!DOCTYPE html>
<html lang="pt-br" ng-app="contatooh">

<head>
    <meta name="viewport" content="width=device-width">
    <meta charset="UTF-8">
    <title>Contatooh</title>
    <link rel="stylesheet" href="../vendor/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="../vendor/bootstrap/dist/css/bootstrap-theme.min.css">
    <link rel="stylesheet" type="text/css" href="../vendor/sweetalert/dist/sweetalert.css">
</head>

<body ng-controller="ContatoController" class="container">      
    <%if (usuarioLogado.lenght > 0) { %>
    <div class="text-right">
        <p> Logado como:
            <span id="usuario-logado">
        <%=usuarioLogado %>
    </span>
    <a href="/logout">Logout</a>
        </p>
    </div>    
    <% } %>

    <!--libs-->
    <script src="../vendor/jquery/dist/jquery.min.js"></script>
    <script src="../vendor/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="../vendor/angular/angular.min.js"></script>
    <script src="../vendor/angular-route/angular-route.min.js"></script>
    <script src="../vendor/angular-resource/angular-resource.min.js"></script>
    <script src="../vendor/sweetalert/dist/sweetalert.min.js"></script>

    <!--app-->
    <script src="../js/app.js"></script>

    <!--controllers-->
    <script src="../js/controllers/contato-controller.js"></script>

    <!--services-->
    <script src="../js/services/contato-service.js"></script>
    <script src="../js/services/interceptor-service.js"></script>

    <!--util-->
    <script src="../js/util/message.js"></script>

</body>

</html>